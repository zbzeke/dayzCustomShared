        <file name="types_Code_Lock.xml" type="types" />
        <file name="types_CP.xml" type="types" />
        <file name="types_GoldNuggets.xml" type="types" />
        <file name="types_Grave.xml" type="types" />
        <file name="types_Heli.xml" type="types" />
        <file name="types_Morty.xml" type="types" />
        <file name="types_MuchCarKey.xml" type="types" />
        <file name="types_MuchStuff.xml" type="types" />
        <file name="types_Mung.xml" type="types" />
        <file name="types_MVS.xml" type="types" />
        <file name="types_dashit.xml" type="types" />
        <file name="types_PsychoGamingVehicles.xml" type="types" />
        <file name="types_RadiationZone.xml" type="types" />
        <file name="types_ToxicZone.xml" type="types" />
        <file name="types_Windstrides.xml" type="types" />
        <file name="types_Zeroy.xml" type="types" />
    </ce>
    <ce folder="again">
        <file name="cfgspawnabletypes_Arma.xml" type="spawnabletypes" />
